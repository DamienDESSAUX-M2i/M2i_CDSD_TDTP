{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Statement1",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::admin-bucket",
        "arn:aws:s3:::manager-bucket",
        "arn:aws:s3:::client-bucket"
      ]
    },
    {
      "Sid": "Statement2",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::client-bucket/*"
      ]
    },
    {
      "Sid": "Statement3",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::admin-bucket/*",
        "arn:aws:s3:::manager-bucket/*"
      ]
    }
  ]
}